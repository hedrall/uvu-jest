      "import": "./dist/jest-to-uvu.js"
    }
  },
  "author": "hedrall",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.15",
    "kleur": "^4.1.4",
    "ts-uvu": "^0.0.9",
    "typescript": "^4.5.5"
  }
}
